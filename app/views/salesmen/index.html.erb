<div class="page-header">
  <h1>Vendedores de <%= @supplier.name %></h1>
</div>
<%= link_to 'Nuevo vendedor', new_supplier_salesman_path, :class => "btn btn-primary pull-right" %>
<div class="row">
  <div class="col-md-5">
    <table class="table">
      <thead>
        <tr>
          <th>Nombre</th>
          <th>Tel√©fono</th>
          <!-- <th>Supplier</th> -->
          <th></th>
          <th></th>
          <th></th>
        </tr>
      </thead>

      <tbody>
        <% @salesmen.each do |salesman| %>
          <tr>
            <td><%= salesman.name %></td>
            <td><%= salesman.phone %></td>
            <!-- <td><%= salesman.supplier_id %></td> -->
            <!-- <td><%= link_to 'Show', supplier_salesman_path(@supplier, salesman) %></td> -->
            <td><%= link_to 'Edit', edit_supplier_salesman_path(@supplier, salesman) %></td>
            <td><%= link_to 'Destroy', supplier_salesman_path(@supplier, salesman), method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
